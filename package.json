{
  "name": "firebase-nest",
  "version": "0.0.14",
  "description": "utility to easily join multiple firebase paths and nested data into a single subscription",
  "keywords": [
    "firebase",
    "nested queries",
    "similar to relay",
    "redux",
    "middleware"
  ],
  "main": "lib/index.js",
  "readmeFilename": "./README.md",
  "tonicExampleFilename": "./examples/tonicExample.js",
  "bugs": {
    "url": "https://github.com/nyura123/firebase-nest/issues"
  },
  "scripts": {
    "test": "babel-node test/nested.spec.js | tap-spec",
    "compile": "babel --plugins transform-object-assign --optional runtime -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
}
